{
  "name": "main-service",
  "version": "1.0.0",
  "description": "Simple blockchain indexer service",
  "main": "dist/index.js",
  "author": "",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "npm run nm src/index.ts",
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon src/index.ts",
    "nm": "nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ",
    "task:archive:dev": "npm run nm src/cli-task.ts archive-task",
    "task:archive": "node dist/cli-task.js archive-task",
    "tester": "npm run nm src/test.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.723.0",
    "@aws-sdk/client-ecs": "^3.723.0",
    "@aws-sdk/client-s3": "^3.726.1",
    "@aws-sdk/client-sqs": "^3.726.0",
    "@aws-sdk/s3-request-presigner": "^3.726.1",
    "@dsnp/parquetjs": "^1.8.5",
    "@prisma/client": "^6.2.1",
    "bip39": "^3.1.0",
    "crypto": "^1.0.1",
    "csv-stringify": "^6.5.2",
    "csv-writer": "^1.6.0",
    "ethereum-hdwallet": "^0.1.2",
    "ethers": "^6.13.5",
    "express": "^4.21.2",
    "fs": "^0.0.1-security",
    "ioredis": "^5.4.2",
    "mime-types": "^2.1.35",
    "nanoid": "^5.0.9",
    "pg": "^8.13.1",
    "stream": "^0.0.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.723.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^5.0.0",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.4.2",
    "@types/pg": "^8.11.10",
    "dotenv": "^16.4.7",
    "nodemon": "^3.1.9",
    "prisma": "^6.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.0"
  },
